{% schema %}
{
  "name": "Icon list",
  "tag": "section",
  "presets": [
    {
      "name": "Icon list",
      "blocks": [
      ]
    }
  ],
  "settings": [
    
  ],
  "blocks": [
    {
      "type":"svg_content",
      "name":"SVG + Content",
      "settings": [
        {
          "type":"text",
          "id":"content",
          "label":"Edit content"
        },
        {
          "type":"radio",
          "id":"icon_type",
          "label":"Icon type",
          "options":[
            {
              "value":"svg",
              "label": "SVG"
            },
            {
              "value":"image",
              "label": "Image"
            }
          ],
          "default": "svg"
        },
        {
          "type":"html",
          "id":"svg",
          "label":"SVG code",
          "info": "Something like <svg....</svg>"
        },
        {
          "type":"image_picker",
          "id":"image",
          "label":"Image icon"
        },
        {
          "type":"text",
          "id":"max_width",
          "label":"Icon max width",
          "default": "100px",
          "info": "E.g: 100px. [How to fill here?](https:\/\/support.arenacommerce.com\/a\/solutions\/articles\/6000256537)"
        }
      ]
    }
  ]
}
{% endschema %}