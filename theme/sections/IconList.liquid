<link rel="stylesheet" href="{{ 'IconList.css' | asset_url }}">

<section class="IconList container">
    <div class="IconList_inner ">

        {% for block in section.blocks %}
        {% case block.type %}
            {% when 'svg_content' %}

           
            <div class="IconList_Item">
                <div class="IconList_Item_pic">
                    {% if block.settings.icon_type == 'svg' %}
                    {{block.settings.svg}}
                    {% endif %}
                    {% if block.settings.icon_type == 'image' %}

                    <img src=" {{ block.settings.image | img_url:'small'}} " />
                        
                    {% endif %}
                    


                    </div>
                <div class="IconList_Item_text">{{block.settings.content}}</div>
            </div>
            {% else %}
                
        {% endcase %}
            
        {% endfor %}

        
