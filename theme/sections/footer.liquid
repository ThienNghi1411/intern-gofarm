<link rel="stylesheet" href="{{ 'footer.css' | asset_url }}">


<section class="footer_NavMenu">
   <div class="footer_NavMenu_inner">
      <div class="footer_NavMenu_Header-iconClose">
         {% render 'icon-close' %}
      </div>
      <div class="footer_NavMenu_content">
  
         <div class="footer_NavMenu_menuLink">
            {% for link in linklists[section.settings.menu].links %}
               {% assign child_list_handle = link.title | handleize %}
               {% if linklists[child_list_handle].links != blank %}
                 <a class="navMenu__menuOption" href="{{ link.url }}">{{ link.title }}</a>

               {% else %}
                 <a  class="navMenu__menuOption" href="{{ link.url }}">{{ link.title }}</a>
               {% endif %}
           {% endfor %}

         </div>
      </div>
   </div>
</section>














<footer class="footer container">
    <div class="footer_Inner">
      <div class="footer-mobile-menu container">

         <div class="moblie-menu_wrap">
               <div class="hambuger_menu_pic">
                  {% render 'icon-hamburger' %}
               </div>
               <div class="hambuger_menu_text">Footer Menu</div>
         </div>
     </div>

        <div class="footer_left">

         
            {%- assign menuList = linklists[section.settings.menu] -%}

              
            <ul class="footer_menu">
                
                 
                    {%- if menuList.links.size > 0 -%} {%- for l in menuList.links -%} {%- render 'link', link: l -%} {%- endfor -%} {%- else -%} {%- for i in (1..5) -%}
                    <li class="menu_text"><a href="javascript:void(0);" class="list-item-link link">Sample menu item</a></li>
                    {%- endfor -%} {%- endif -%}
            </ul>
            
