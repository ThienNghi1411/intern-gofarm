            {% style %}
                .productPage__imgCont{
                    display: flex;
                    flex-direction: row-reverse;
                    gap: 10px;
                }
                .productPage__subImgList{
                    display: block;
                }

                .productPage__content{
                    flex-direction: row;
                    gap: 20px;
                }

                .productPage__titleAndShare{
                   width: 50%;
                   flex-direction: column;
                   justify-content: flex-start;
                }
                .productPage__socialMedia{
                    padding: 50px 0px 25px 0px;
                }
                .productPage__offer{
                    flex-direction: column;
                    gap: 10px
                }
                .productPage__priceCont{
                    width: 50%;
                }
                .productPage__priceContWrap{
                    padding: 30px;
                    background: #F1FAF0;
                    border-radius: 5px;
                    display: flex;
                    flex-direction: column;
                    gap: 25px;
                    margin-bottom: 20px;
                }
                .productPage__priceProduct, .productPage__sizeCont{
                    margin: 0;
                }

                .productPage__sizeOption{
                    font-weight: 600;
                    font-size: 13px;
                    line-height: 1.4;
                    color: #4D4D4D;
                }
                .productPage__quantityFieldCont{
                    display: block;
                }
                .productPage__quantityField{
                    flex-direction: column;
                    align-items: flex-start;
                    margin-bottom: 20px;
                }
                .productPage__quantityAdjust{
                    background: #fff;
                }
            {% endstyle %}
        {% endif %}
        <script id="dataProduct" type="application/ld+json">
            {
              "dataProduct" : {{ product.variants | json }}
            }
        </script>
    </div>
</section>



{% schema %}
{
  "name": "Product Page",
  "tag": "section",
  "presets": [
    {
      "name": "Product Page"
    }
  ],
  "settings":[
    {
        "type":"header",
        "content":"Product Image"
    },
    {
        "type":"select",
        "id":"bannerStyle",
        "label":"Choose style of design: ",
        "options":[
            {
                "value":"1",
                "label":"Style 1"
            },
            {
                "value":"2",
                "label":"Style 2"
            }
        ],
        "default":"1"
    },
    {
        "type": "image_picker",
        "id": "banner",
        "label": "banner image"
    },
    {
        "type":"product",
        "id":"product",
        "label": "pick product"
    }
  ]

}
{% endschema %}